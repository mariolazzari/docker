docker run --name sqlserver -e "ACCEPT_EULA=Y" -e "MSSQL_SA_PASSWORD=4zl3A,75" -p 1433:1433 -v c:/Docker/sqlserver/data:/var/opt/mssql/data -v c:/Docker/sqlserver/log:/var/opt/mssql/log -v c:/Docker/sqlserver/secrets:/var/opt/mssql/secrets -d mcr.microsoft.com/mssql/server:2019-latest

docker run --name sql2019 -e "ACCEPT_EULA=Y" -e "MSSQL_SA_PASSWORD=4zl3A,75" -p 1433:1433 -v -d mcr.microsoft.com/mssql/server:2019-latest

https://blog.sqlauthority.com/2019/04/20/sql-server-docker-volume-and-persistent-storage/

docker run -e "ACCEPT_EULA=Y" -e "SA_PASSWORD=4zl3A,75" -p 1433:1433 --name sql2019 -v sql2019:/var/opt/mssql -d mcr.microsoft.com/mssql/server:2019-latest

docker cp anod sql2019:/var/opt/mssql/data/anod.bak
